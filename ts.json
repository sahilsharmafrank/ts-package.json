{
  "name": "postgres-crud",
  "version": "1.0.0",
  "description": "simple crud",
  "main": "dist/api/server.js",
  "scripts": {
    "build-ts": "tsc",
    "build": "npm run build-ts && npm run lint",
    "debug": "npm run build && npm run watch-debug",
    "dev": "npm run build && npm run watch-debug",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve-debug": "nodemon --inspect=0.0.0.0:9230 dist/api/server.js",
    "serve": "node dist/api/server.js",
    "start": "npm run serve",
    "test": "jest --forceExit --coverage --verbose",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon dist/api/server.js",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "prod": "npx sequelize-cli db:migrate && node server.js",
    "consumer": "npm run build && node dist/consumers/awssqs.js",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-tslint-format": "prettier-tslint fix '**/*.ts'",
    "tsc:v": "tsc -v"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "node",
    "postgres",
    "typescript"
  ],
  "author": "Hyperion",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.32.0",
    "async": "^3.2.1",
    "aws-s3-multipart-copy": "^1.0.12",
    "aws-sdk": "^2.991.0",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.15",
    "concurrently": "^6.2.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "fast-xml-parser": "^3.20.3",
    "inversify": "^5.1.1",
    "moment-timezone": "^0.5.33",
    "odata-parser": "^1.4.1",
    "pg": "^8.6.0",
    "querystring": "^0.2.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.4",
    "sequelize-cli": "^6.2.0",
    "sqs-consumer": "^5.6.0",
    "strformat": "0.0.7",
    "url": "^0.11.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.12",
    "@types/node": "^15.12.5",
    "@types/uuid": "^8.3.1",
    "@types/validator": "^13.1.4",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "prettier-tslint": "^0.4.2",
    "sequelize-auto": "^0.8.3",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.4"
  }
}
